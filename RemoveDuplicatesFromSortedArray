class Solution {
public:
    int removeDuplicates(int A[], int n) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
        int nSize = 0;
        if (n==0){
            return 0;
        }
        for (int i = 1; i<n; i ++){
            if(A[nSize]!=A[i]){
                nSize++;
                A[nSize] = A[i];
            }
        }
        return nSize+1;
    }
};
